\section{Längen und Volumenausdehnung}
\begin{multicols}{2}
 \[\rho\left(T\right) = \rho_0 \left( 1 - \beta \left(T - T_0\right)\right)\]
 \[L\left(T\right) = L_0 \left( 1 + \alpha \left(T - T_0\right)\right)\]
\vfill
 \[V\left(T\right) = V_0 \left( 1 + \gamma \left(T - T_0\right)\right)\]
 \[\text{mit } 3\alpha \approx \gamma\]
\end{multicols}

\section{Wärmekapazität}
\begin{alignat*}{2}
\text{Wärmekapazität} 	&\quad\quad& C &= m \cdot c \\
\text{massenspezifisch} &\quad\quad& c &= C/m \\
\text{molspezifisch} 	&\quad\quad& c_{mol} &= C/n = c \cdot m_{mol}
\end{alignat*}

